<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:igWindows="http://infragistics.com/Windows"
                    xmlns:igDP="http://infragistics.com/DataPresenter"
                    xmlns:igEditors="http://infragistics.com/Editors"
                    
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:RazerConverters="clr-namespace:RazerConverters;assembly=RazerConverters">



    <RazerConverters:cCreditStatusConverter x:Key="CreditStatusConverter"/>
    <RazerConverters:cRowHighlightConverter x:Key="RowHighlightConverter"/>
    <RazerConverters:cHideNegativesConverter x:Key="HideNegatives"/>
    <RazerConverters:DateTimeToNullableDateTimeConverter x:Key="NullDateConverter" />


    <igDP:FieldLayout x:Key="BCFDetail" x:Shared="False">
        <igDP:Field Name="BCF_detail_id" Label="Detail ID" Width="120">
            <!--Code to do a drop down box in a grid or can be used as a text lookup for a value field for a non editable grid-->

        </igDP:Field>
        <igDP:Field Name="BCF_number" Visibility="Collapsed" Width="70">

        </igDP:Field>
        <igDP:Field Name="invoice_to_be_credited" Label="Invoice to Credit" Width="150">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamTextEditor}"    />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="first_billing_period_effected" Label="First Service Period Effected" Width="180">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamDateTimeEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="last_billing_period_effected" Label="Last Service Period Effected" Width="180">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamDateTimeEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="credit_amount" Label="Credit Amount" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings>
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[credit_amount].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="document_id" Label="Adjustment Number" Width="150">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamTextEditor}"    />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="rebill_flag" Label="Credit/Rebill" Width="150">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>

        <!-- Add item_description and line_desc to BCF folder -->

        <igDP:Field Name="inv_line_id" Visibility="Collapsed" Width="70">
        </igDP:Field>
        <igDP:Field Name="item_description" Label="Product Item" Width="150">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="line_desc" Label="Line Description" Width="150">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
    </igDP:FieldLayout>

