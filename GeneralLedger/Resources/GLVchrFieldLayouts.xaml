                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[actual_amt_deferred].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="amt_override" Label="New Disputed Amount" Width="160">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[amt_override].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>







        <igDP:Field Name="percent_delivered" Label="Percentage Complete" Width="160">
            
                <igDP:Field.Settings>
                    <igDP:FieldSettings 
			   EditorStyle="{DynamicResource RateStylePercentCalc}" >
                    </igDP:FieldSettings>
                </igDP:Field.Settings>

            
        </igDP:Field>
        <igDP:Field Name="completed_contract" Label="Completed Contract" Width="160">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>            
        </igDP:Field>
        <igDP:Field Name="code_value" Label="Deferral Type" Width="140">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="rule_id" Label="Rule ID" Width="60" >
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="description" Label="Description" Width="225">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="po_number" Label="PO Number" Width="225" Visibility="Collapsed">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True">
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>





        <igDP:Field Name="date_closed" Label="Date Closed" Width="100" >
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
      
        <igDP:Field Name="recog_period" Label="Recog Period" Width="100" Visibility="Collapsed">
