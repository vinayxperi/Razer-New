            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>    
    </igDP:FieldLayout>

    <igDP:FieldLayout x:Key="DefPoolSmry" x:Shared="False">
        <igDP:Field Name="pool_id" Label="Pool ID" Width="90" Visibility="Collapsed"></igDP:Field>
        <igDP:Field Name="recv_doc" Label="Document ID" Width="90"></igDP:Field>
        <igDP:Field Name="product_code" Label="Product Code" Width="110"></igDP:Field>
        <igDP:Field Name="amount_deferred" Label="Amount Deferred" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[amount_deferred].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="amount_total" Label="Amount Total" Width="110">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[amount_total].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="currency_code" Label="Currency Code" Width="90"></igDP:Field>
        <igDP:Field Name="receivable_account" Label="Customer ID"   Width="110"></igDP:Field>
        <igDP:Field Name="account_name"   Label="Customer" Width="130"></igDP:Field>
        <igDP:Field Name="account_code" Label="Revenue Account" Width="140"></igDP:Field>
        <igDP:Field Name="pool_detail_id" Label="Detail Id" Width="90" Visibility="Collapsed"></igDP:Field>

    </igDP:FieldLayout>

    <igDP:FieldLayout x:Key="DefPoolDetail" x:Shared="False">
        <igDP:Field Name="reason_desc" Label="Reason " Width="140"></igDP:Field>
        <igDP:Field Name="recog_period" Label="Recog Period" Width="100"></igDP:Field>
        <igDP:Field Name="amt_deferred" Label="Amount Deferred" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[amt_deferred].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="pool_detail_id" Label="Detail Id" Width="90" Visibility="Collapsed"></igDP:Field>
        <igDP:Field Name="pool_id" Label="Pool Id" Width="70" Visibility="Collapsed"></igDP:Field>
        <igDP:Field Name="def_account_code" Label="Def Account" Width="70" Visibility="Collapsed"></igDP:Field>

    </igDP:FieldLayout>

    <igDP:FieldLayout x:Key="DefPoolMgmt" x:Shared="False">
