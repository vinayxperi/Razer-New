    </igDP:FieldLayout>

    <igDP:FieldLayout x:Key="DefPoolMgmtDetail" x:Shared="False">
        <igDP:Field Name="pool_id" Label="Reason" Width="70" Visibility="Collapsed">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False">                  
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="pool_reason_id" Label="Reason" Width="70" Visibility="Collapsed"></igDP:Field>
