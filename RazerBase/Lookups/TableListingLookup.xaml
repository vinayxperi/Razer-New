<local:DialogBase x:Class="RazerBase.Lookups.TableListingLookup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RazerBase"
             mc:Ignorable="d" 
             Height="500" Width="880"
             Background="{StaticResource ApplicationBackground}"
             WindowStartupLocation="CenterOwner" >
    <Grid>        
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="355"/>            
            <RowDefinition Height="53*" />            
        </Grid.RowDefinitions>     
        <TextBlock Text="Available Maintenance Tables" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="14" Foreground="AntiqueWhite" FontWeight="Bold" Grid.Row="0" />
        <local:ucBaseGrid x:Name="idgTableListing" Grid.Row="1"></local:ucBaseGrid>
        <Button Style="{StaticResource GlassButtonSmall}" Content="OK" Height="23" HorizontalAlignment="Right" VerticalAlignment="Top" IsDefault="True" Name="btnOK" Grid.Row="2" Width="75" Click="btnOK_Click" Margin="422,23,101,10" />
        <Button Style="{StaticResource GlassButtonSmall}" Content="Cancel" Height="23" HorizontalAlignment="Right" Name="btnCancel" Grid.Row="2" VerticalAlignment="Top" Width="75" IsCancel="True" Margin="0,23,11,0" Click="btnCancel_Click" />
    </Grid>
</local:DialogBase>
