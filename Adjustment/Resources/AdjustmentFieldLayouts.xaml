    <RazerConverters:BitToBoolConverter x:Key="BooleanConverter"/>
    <!--Used for checkboxes in grids when you need immediate response-->
    <Style TargetType="{x:Type igDP:CellValuePresenter}" x:Key="ImmediateCheckBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <CheckBox
                                    IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Value, Mode=TwoWay,
                                                Converter={StaticResource BooleanConverter}, UpdateSourceTrigger=PropertyChanged}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


   


