    <igDP:FieldLayout x:Key="ContractCompany" x:Shared="False">
        <igDP:Field Name="company_id" Label="Company Name" Width="180">
            <!--Code to do a drop down box in a grid or can be used as a text lookup for a value field for a non editable grid-->
                <igDP:Field.Settings>
                    <igDP:FieldSettings EditorType="{x:Type igEditors:XamComboEditor}" >
                        <igDP:FieldSettings.EditorStyle>
                            <Style TargetType="{x:Type igEditors:XamComboEditor}">
                            <Setter Property="ItemsProvider" Value="{Binding Path=cmbCompanyGridCombo, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type UserControl}, AncestorLevel=2}}"/>
                        </Style>
                        </igDP:FieldSettings.EditorStyle>
                    </igDP:FieldSettings>
                </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="percentage" Label="Percentage" Width="70">
            
        </igDP:Field>
        <igDP:Field Name="primary_company_flag" Label="Primary Company" Width="100">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="contract_id" Label="Contract ID" Width="0" Visibility="Collapsed"  ></igDP:Field>
    </igDP:FieldLayout>

