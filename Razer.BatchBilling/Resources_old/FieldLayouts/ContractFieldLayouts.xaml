    <igDP:FieldLayout x:Key="ContractReviewDates" x:Shared="False">
        <igDP:Field Name="contract_review_id" Label="Contract Review ID" Width="130"  >
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False"></igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="contract_id" Visibility="Collapsed" Width="70">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True"></igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="review_date" Label="Review Date" Width="110">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamDateTimeEditor}" >
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamDateTimeEditor}">
                            <Setter Property="ValueToDisplayTextConverter" Value="{StaticResource NullDateConverter}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="review_reason" Label="Review Reason" Width="300">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamTextEditor}" AllowEdit="True" >
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamTextEditor}">
                            <Setter Property="TextWrapping" Value="WrapWithOverflow"/>
                            <Setter Property="ValueConstraint">
                                <Setter.Value>
                                    <igEditors:ValueConstraint MaxLength="500"/>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>

        </igDP:Field>
        <igDP:Field Name="date_reviewed" Label="Date Reviewed" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamDateTimeEditor}" >
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamDateTimeEditor}">
                            <Setter Property="ValueToDisplayTextConverter" Value="{StaticResource NullDateConverter}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="reviewed_by" Label="Reviewed By" Width="110">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True"></igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="review_result" Label="Review Result" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True"></igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="targeted_reviewer" Label="Targeted Reviewer" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamComboEditor}" >
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamComboEditor}">
                            <Setter Property="ItemsProvider" Value="{Binding Path=cmbReviewer, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type UserControl}, AncestorLevel=2}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
                </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="reviewed_flag" Label="Reviewed" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="review_notice_date" Label="Notice Date" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings EditorType="{x:Type igEditors:XamDateTimeEditor}" >
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamDateTimeEditor}">
                            <Setter Property="ValueToDisplayTextConverter" Value="{StaticResource NullDateConverter}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="review_frequency" Label="Review Frequency" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True"></igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="auto_close_flag" Label="Auto Close" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamCheckEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        
