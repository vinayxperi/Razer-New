    <igDP:FieldLayout x:Key="TFDetail" x:Shared="False">
        <igDP:Field Name="tf_invoice_id" Label="Detail ID" Width="60" Visibility="Collapsed" >
        </igDP:Field>
        <igDP:Field Name="tf_number" Label="TF Nmber" Width="100" Visibility="Collapsed">
        </igDP:Field>
        <igDP:Field Name="invoice_to_be_credited" Label="Invoice to Credit" Width="100">
