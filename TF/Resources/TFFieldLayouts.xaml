            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="cs_id" Label="Location" Width="80" >
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="psa_city" Label="PSA City" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="psa_state" Label="PSA State" Width="70" >
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="product_code" Label="Product Code" Width="90">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="False" />
            </igDP:Field.Settings>
        </igDP:Field>
              
        <igDP:Field Name="first_billing_period_effected" Label="Service Period Start" Width="115">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamDateTimeEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="last_billing_period_effected" Label="Service Period End" Width="115">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamDateTimeEditor}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="credit_amount" Label="Credit Amount" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings>
                    <igDP:FieldSettings.EditorStyle>
                        <Style TargetType="{x:Type igEditors:XamNumericEditor}" >
                            <Setter Property="Format" Value="$###,##0.00;($###,##0.00)"  />
                            <Setter Property="Foreground" Value="{Binding Path=Cells[credit_amount].Value, Converter={StaticResource NegativeToRedConverter}}"/>
                        </Style>
                    </igDP:FieldSettings.EditorStyle>
                </igDP:FieldSettings>
            </igDP:Field.Settings>
        </igDP:Field>
        
        <igDP:Field Name="document_id" Label="Adjustment Number" Width="120">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="True" EditorType="{x:Type igEditors:XamTextEditor}"    />
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="rebill_flag" Label="Credit/Rebill" Width="80">
            <igDP:Field.Settings>
                <igDP:FieldSettings AllowEdit="true" EditorType="{x:Type igEditors:XamCheckEditor}" CellValuePresenterStyle="{StaticResource ImmediateCheckBox}"/>
            </igDP:Field.Settings>
        </igDP:Field>
        <igDP:Field Name="inv_line_id" Label="Line ID" Width="100" Visibility="Collapsed"></igDP:Field>

